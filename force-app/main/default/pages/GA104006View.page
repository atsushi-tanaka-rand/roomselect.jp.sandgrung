        <script>
            var canvas;
            var context;
            var isDrawing = false;              // メモ入力中判定
            var prevX, prevY, currX, currY = 0; // メモ入力の座標情報
            var isDrawMode = false;             // メモ入力On/Off
            var isMemoDisp = false;             // メモ表示非表示
            var widthSpace = 250;               // メモ記載用の余白幅
            var supportTouch = 'ontouchend' in document; //タッチイベントが可能か判断する
            // メモ記載領域初期化処理
            function initCanvas() {
                canvas = document.getElementById("floorplanPhotoMemoCanvas");
                canvas.addEventListener("mousedown", drawStart, false);
                canvas.addEventListener("mousemove", draw, false);
                canvas.addEventListener("mouseup", drawStop, false);
                canvas.addEventListener("mouseout", drawStop, false);
                canvas.addEventListener("touchstart", drawStart, false);
                canvas.addEventListener("touchmove", draw, false);
                canvas.addEventListener("touchend", drawStop, false);
                context = canvas.getContext("2d");  // 2d描画コンテキスト
                context.strokeStyle = "black";      // 線の色設定
                context.lineWidth = "5";            // 線の太さ設定
            }
            // 間取り図用初期化処理
            function initFloorplanPhoto() {
                // メモ入力off
                document.getElementById("{!$Component.IsFloorplanPhotoDrawMode}").checked = false;
                isDrawMode = false;
                // メモ表示off
                document.getElementById("{!$Component.IsFloorplanPhotoMemoDisp}").checked = false;
                isMemoDisp = false;
                // メモなし表示、メモあり非表示
                document.getElementById("{!$Component.floorplanPhotoImageBlock}").style.display = '';
                document.getElementById("{!$Component.floorplanPhotoMemoImageBlock}").style.display = 'none';
