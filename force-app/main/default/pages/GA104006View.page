                if (isMemoDisp) {
                    floorplanPhotoMemoBase.style.left = '0px';
                } else {
                    floorplanPhotoMemoBase.style.left = widthSpace + 'px';
                }
                // 間取り図からメモ入力領域の幅・高さを求める
                var canvasHeight = floorplanPhotoMemoBase.height;   // 高さ
                var canvasWidth = floorplanPhotoMemoBase.width;     // 幅
                if (!isMemoDisp) {
                    canvasWidth = canvasWidth + widthSpace + widthSpace;    // メモなしを元とする場合は左右に余白
                }
