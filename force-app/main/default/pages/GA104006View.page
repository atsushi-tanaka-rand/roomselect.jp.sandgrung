                            // 解析確認メッセージ
                            var retInt = await showConfirmCloseMultiple("{!MSG_PROP_I_ROOMLAYOUT_AUTOANALYSIS}","使用して作図","使用せずに作図");
                            if (retInt == 1 ) {
                                // 解析
                                roomLayoutAutoAnalysisFlg = 'True';      
                            }else if(retInt == 2){
                                // 解析しない
                                roomLayoutAutoAnalysisFlg = 'False'; 
                            }else if(retInt == 0){
                                // 閉じる
                                return;
                            }
                            //▲問合せNo0549で変更
                        }

                        blockUi();

                        // 間取り図アプリ用フォームを作成
                        form = document.createElement('form');
                        form.id = 'input_form';
                        form.name = 'input_form';
                        document.body.appendChild(form);

                        // 間取り図APIキーのPost送信用設定
                        apiKeyPost = document.createElement('input');
                        apiKeyPost.id = 'apiKey';
                        apiKeyPost.type = 'text';
                        apiKeyPost.name = 'apiKey';
                        apiKeyPost.size = '50';
                        apiKeyPost.value = apiKey; 
                        document.input_form.appendChild(apiKeyPost);  

                        // 間取り図のファイルパス設定
                        var filePath = '';
                        var roomlayoutFilepath ="{!screenPropertyRecord.BuildingForeignID}/{!screenPropertyRecord.ForeignID}/"; 
                        // 間取り図ファイル有無
                        if('{!floorplanPhotoData.photoCount}' == '0'){
                            // 間取り図ファイル名の生成
                            filePath = getS3FileName{!floorplanPhotoGD901001Parameter.photosubType}(0) + '.jpg';
                        }else{
                            // 間取り図パスからファイル取得
                            filePath = "{!floorplanPhotoData.photoDataList[0].FileName}";
                        }
                        roomlayoutFilepath += filePath;
                        document.getElementById("{!$Component.roomLayoutFileName}").value = filePath;       // 間取り図のファイル名を登録用に保持

                        // 間取り図ファイルパスPost送信用設定
                        roomlayoutFilepathPost = document.createElement('input');
                        roomlayoutFilepathPost.id = 'roomlayoutFilepath';
                        roomlayoutFilepathPost.type = 'text';
                        roomlayoutFilepathPost.name = 'roomlayoutFilepath';
                        roomlayoutFilepathPost.size = '150';
                        roomlayoutFilepathPost.value = roomlayoutFilepath; 
                        document.input_form.appendChild(roomlayoutFilepathPost);

                        // 間取り図自動解析フラグのPost送信用設定
                        autoAnalysisFlgPost = document.createElement('input');
                        autoAnalysisFlgPost.type = 'checkbox';
                        autoAnalysisFlgPost.id = 'autoAnalysisFlg';
                        autoAnalysisFlgPost.name = 'autoAnalysisFlg';
                        autoAnalysisFlgPost.value = roomLayoutAutoAnalysisFlg; 
                        autoAnalysisFlgPost.checked = roomLayoutAutoAnalysisFlg; 
                        document.input_form.appendChild(autoAnalysisFlgPost);    

                        // アクセス元のOriginが取得用にfqdn追加  問合せNo0462で追加
                        fqdn = document.createElement('input');
                        fqdn.type = 'text';
                        fqdn.id = 'fqdn';
                        fqdn.name = 'fqdn';
                        fqdn.size = '150'; 
                        fqdn.value = location.host; 
                        document.input_form.appendChild(fqdn);    

                        // 間取り図作成アプリを開く
                        var callback_id = 'callback_cad';
                        popup = window.open("about:blank",callback_id,"width="+window.innerWidth+",height="+window.innerHeight+",scrollbars=yes");
                        document.input_form.target = callback_id;
                        document.input_form.method = "post";
                        document.input_form.action = madoriUrl;
                        document.input_form.submit();
                        window.addEventListener("message", receiveMessage);

                        console.log("間取り図APIキー="+apiKey);
                        console.log("間取り図アプリURL="+madoriUrl);
                        console.log("間取り図ファイルパス="+roomlayoutFilepath);
                        console.log("間取り図解析フラグ="+roomLayoutAutoAnalysisFlg);
                        console.log("間取り図fqdn="+fqdn.value);
                        console.log(autoAnalysisFlgPost);

                        // 間取り図作成画面呼び出し用のタグを削除する。
                        document.input_form.removeChild(apiKeyPost);
                        document.input_form.removeChild(roomlayoutFilepathPost);
                        document.input_form.removeChild(autoAnalysisFlgPost);
                        document.input_form.removeChild(fqdn);      // 問合せNo0462で追加                    
                        document.body.removeChild(form);
                    } catch(e) {
                         if (typeof popup != "undefined") {
                            popup.close();      // 例外エラー時に間取り図を閉じる。
                        }
                        unblockUi();
                        // 間取り図アプリの情報が残っていれば削除する。
                        if(document.input_form){
                            var del_form = document.getElementById("input_form");
