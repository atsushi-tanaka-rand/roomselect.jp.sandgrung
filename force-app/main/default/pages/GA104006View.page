                            del_autoAnalysisFlgPost.parentNode.removeChild(del_autoAnalysisFlgPost);
                        }
                        var message = "{!MSG_COM_E_SYSTEM_EXCEPTION}";
                        message = message.replace('{0}', e);
                        showAlert(message);
                    }
                }

                // メッセージ受け取り関数
                function receiveMessage(event) {
                    console.log('------------間取り図アプリ終了後の情報を取得するためにログを記載--------------');
                    console.log(event.origin);
                    console.log(event.data);
                    console.log(navigator.platform);

                    if (navigator.platform != "MacIntel"){          
                        // オリジンエラーになった際に処理を終了することなく続行する
                        if (('{!roomLayoutApiUrl}').indexOf(event.origin) < 0) {
                            // ↑ 送信元のオリジンの不一致
                            var message = "{!MSG_COM_E_SYSTEM_EXCEPTION}";
                            message = message.replace('{0}', '送信元のオリジンの不一致');
                            showAlert(message);
                            // addEventListenerにて登録されたイベントリスナーの削除
                            window.removeEventListener('message', receiveMessage);
                            unblockUi();
                            return;
                        }             
                    }

                    // addEventListenerにて登録されたイベントリスナーの削除
                    window.removeEventListener('message', receiveMessage);
                    // 間取り図アプリからの情報で処理を判断
                    if(event.data){
                        // 物件写真保存
                        document.getElementById("{!$Component.roomLayoutNewFileName}").value = event.data;  //問合せNo0573で追加
                        initFloorplanPhoto();
                        appAfterSaveFloorplanPhoto();  
                    }
                    unblockUi();
                }

                // 物件登録を閉じた際に間取り図アプリも閉じる（遷移も含む）。
                window.addEventListener('unload', function(event) {
                    if (typeof popup != "undefined") {
                        popup.close();
                    }
                });

                // 問合せNo0491で追加
                // 間取り図アプリで作成されたか判断（jsonファイル有無） 
                function checkeJsonFile() {

                    try {
                        // 間取り図が1件もなければtrueで返す
                        if('{!floorplanPhotoData.photoCount}' == '0'){
                            console.log("間取り図0件");
                            return true;
                        }
                        var filePath = "{!floorplanPhotoData.photoDataList[0].FilePath}";
                        var extension = filePath.lastIndexOf('.');      // 拡張子の位置を取得
                        if(extension == -1 ){
                            // 拡張子が取れない場合は無扱い
                            return false;
                        }

                        // Jsonファイルパスに成形
                        var jsonFilePath = filePath.substring(0,extension) + ".json";
                        console.log("jsonFilePath="+jsonFilePath);
                        
                        // XMLHttpRequestを設定
                        var xhr = new XMLHttpRequest();
                        xhr.open('GET', jsonFilePath,false);
                        // キャッシュを無効にする
                        xhr.setRequestHeader('Pragma', 'no-cache');
                        xhr.setRequestHeader('Cache-Control', 'no-cache');
                        xhr.setRequestHeader('If-Modified-Since', 'Thu, 01 Jun 1970 00:00:00 GMT');

                        var isJsonExistenceFlg = false;
                        xhr.onreadystatechange  = function () {
                            if (xhr.readyState == 4 && ((xhr.status >= 200 && xhr.status < 300) || (xhr.status == 304))) {
                                // jsonファイル有
                                isJsonExistenceFlg = true;
                            }
                        };

                        // データ送信
                        xhr.send(null);
                        console.log("isJsonExistenceFlg="+isJsonExistenceFlg);
                        return isJsonExistenceFlg;
                        
                    } catch(e) {
                        console.log("例外エラー"+e);
                        return false;
                    }
                }
                
            </script>
