                            var message = "{!MSG_COM_E_SYSTEM_EXCEPTION}";
                            message = message.replace('{0}', '間取り図アプリの起動パラメータが設定されていません。');
                            showAlert(message);
                            return;
                        }

                        // 問合せNo0491で追加
                        // 間取り図自動解析フラグのPost送信用設定
                        var roomLayoutAutoAnalysisFlg = '{!roomLayoutAutoAnalysisFlg}';			// 間取り図自動解析フラグセット

                        // jsonファイルチェック
                        var isJsonFlg = checkeJsonFile();
                        // jsonあるもしくは間取り図自動解析フラグが立っていない場合、解析しない
                        if(isJsonFlg || roomLayoutAutoAnalysisFlg == 'false'){
                            roomLayoutAutoAnalysisFlg = 'False';
                        }else{
