        <!-- 共通処理 -->
        <script>
            function getJQueryObjectByApexTagId(Id) {
                return $('[id$=":'+Id+'"]');
            }
            // チェック状態を元に活性/非活性制御
            function setEnabledFromChecked(checked, elements) {
                if (checked == true) {
                    // 活性化
                    setDisabledElements(elements, false);
                } else {
                    // 入力値クリア
                    setValueElements(elements, '');
                    // 非活性化
                    setDisabledElements(elements, true);
                }
            }
            // 複数要素のreadOnlyプロパティ一括設定
            function setReadOnlyElements(elements, value) {
                for (var i=0; i < elements.length; i++) {
                    elements[i].readOnly = value;
                }
            }
            // 複数要素のdisableプロパティ一括設定
            function setDisabledElements(elements, value) {
                for (var i=0; i < elements.length; i++) {
                    elements[i].disabled = value;
                }
            }
            // 複数要素のvalueプロパティ一括設定
            function setValueElements(elements, value) {
                for (var i=0; i < elements.length; i++) {
                    elements[i].value = value;
                }
            }
            // 複数要素のcheckedプロパティ一括設定
            function setCheckedElements(elements, value) {
                for (var i=0; i < elements.length; i++) {
                    elements[i].checked = value;
                }
            }
            // チェックボックス真偽値設定
            function setCheckBoxChecked(checkBoxId, checked) {
                getJQueryObjectByApexTagId(checkBoxId).prop('checked', checked);
            }
            // チェックボックス（.Choice_Box）変更時の単一選択処理
            function onchangeChoiceBox(CheckObject) {
                // Idの最終階層を取得
                var id = CheckObject.id.split(':');
                var searchId = id[id.length-1];
                // 同階層の同Idチェックボックス取得
                var checkList = $(CheckObject).siblings('input[id$="'+searchId+'"]');
                // 選択変更したチェックボックス以外を未選択に変更
                for (var i=0; i < checkList.length; i++) {
                    if (checkList[i].id!=CheckObject.id) {
                        checkList[i].checked = false;
                    }
                }
            }
            // ＜apex:radio＞の選択中の値を取得
            function getRadioChoice(radioId) {
                // ラジオボタン＜apex:radio＞のinput[type=radio]の要素を取得
                var radioItem = getRadioObjects(radioId);
                // 選択中の表示名を取得し返却
                for (var i=0; i < radioItem.length; i++) {
                    if (radioItem[i].checked) {
                        return radioItem[i].value;
                    }
                }
                return '';
            }
            // ＜apex:radio＞のinput[id&=radio]選択中の値を取得
            function getRadioObjects(radioId) {
                // ラジオボタン＜apex:radio＞のブロック要素を取得
                var radioBlock = getJQueryObjectByApexTagId(radioId);
                // ラジオボタン＜apex:radio＞のブロックの中からinput[type=radio]の要素を取得
                return radioBlock.find('input[type=radio]');
            }

            // リストのコントローラ値渡し用チェックボックスをチェックさせる
            function setBackCheckList(checkList,checkList2){

                for(var i=0; i < checkList.length; i++){
                    if (checkList[i].checked) {　　　　
                        checkList2[i].checked = true;
                    }else{　　　　　　　　　　　　　　
                        checkList2[i].checked = false;
                    }
                }
            }
        </script>
        <script>
            // 料金単位の入力制御
            function setFeeUnitInputRule(radioId, inputId) {
                var input = getJQueryObjectByApexTagId(inputId).get(0);
                correctFeeUnitInputRules(input, radioId);
            }
            // 料金単位の入力規則制御
            function correctFeeUnitInputRules(input, radioId) {
                var feeUnit = getRadioChoice(radioId);
                switch (feeUnit) {
                    case "{!FEEUNIT_YEN}" :         // 円
                        correctInputRules(input, 'currency', getFeeUnitMaxYenLength(radioId), 0);
                        break;
                    case "{!FEEUNIT_MONTH}" :       // ヵ月
                        correctInputRules(input, 'currency', 2, 1, 99.9);
                        break;
                    case "{!FEEUNIT_RATE}" :        // %
                        if(input.getAttribute("id").indexOf('ADFeeInput') > 0){
                            // 広告料
                            correctInputRules(input, 'currency', 3, 0, 999);    
                        }else{
                            correctInputRules(input, 'currency', 3, 0, 100);
                        }
                        break;
                    case "{!FEEUNIT_ACTUALCOST}" :  // 実費
                        correctInputRules(input, 'currency', 0, 0);
                        break;
                }
            }
            // 料金単位の「円」時の最大桁数取得
            function getFeeUnitMaxYenLength(radioId) {
                switch (radioId) {
                    case 'SecurityDepositUnitCheck' :       // 敷金
                        return 7;
                    case 'RenewalFeeUnitCheck' :            // 更新料
                        return 7;
                    case 'DepositUnitCheck' :               // 保証料金
                        return 7;
                    case 'KeyMoneyUnitCheck' :              // 礼金
                        return 7;
                    case 'CancellationActualFeeUnitCheck' : // 敷引・償却
                        return 7;
                    case 'ParkingDepositUnitCheck' :        // 駐車場保証料金
                        return 6;
                    case 'ParkingCompoundingFeeUnitCheck' : // 駐車場手数料
                        return 6;
                    case 'CleaningFeeUnitCheck' :           // 定額修繕費
                        return 7;
                    case 'ADFeeUnitCheck' :                 // 広告料
                        return 7;
                    default :
                        return 7;
                }
            }
        
