            // 画像結合
            function saveFloorplanMemoImage() {

                // 保存画像用のCANVAS作成
                var retCanvas = document.getElementById("joinCanvas");
                retCanvas.height = document.getElementById("floorplanPhotoMemoCanvas").height;
                retCanvas.width = document.getElementById("floorplanPhotoMemoCanvas").width;
                var retContext = retCanvas.getContext("2d");

                // ベースとなる間取り図の画像の読込
                var joinBaseImage = new Image();
                var element = document.getElementById("{!$Component.floorplanPhotoMemoBase}");
               
                joinBaseImage.crossOrigin = "Anonymous";
                joinBaseImage.src = element.src;
                var filePashindex = element.src.indexOf('?');
                // 画像ファイルのキャッシュ対応で設定した文字が設定されていればそれを省いたファイルパスをセットする。
                if(filePashindex > 0){
                    joinBaseImage.src = element.src.substring(0,filePashindex);
                }
                console.log(element.src);
                joinBaseImage.addEventListener("load", function() {
                    var leftSpace = 0;
                    if (!isMemoDisp) {
                        leftSpace = widthSpace;
                    }
                    retContext.drawImage(joinBaseImage, leftSpace, 0, element.width, element.height);
                    // メモ入力内容の追加読込
                    var joinImage = new Image();
                    // joinImage.crossOrigin = "Anonymous";
                    joinImage.src = context.canvas.toDataURL();
                    joinImage.addEventListener("load", function() {
                        retContext.drawImage(joinImage, 0, 0);

                        // 画像情報の変換（canvas⇒blob）
                        retContext.canvas.toBlob(
                            function(blob){
                                var blobArray = [blob];
                                // 画像情報の変換（blob⇒file）
                                var file_property_bag = {
                                    type: "image/jpeg"          // コンテンツタイプを設定
                                    ,lastModified: Date.now()   // 最終更新日時を設定（単位：ミリ秒）
                                };
                                var newMemoFile = new File(blobArray, 'newFloorplanPhotoMemo.jpg', file_property_bag);
                                // ファイルのアップロード
                                var fileList = [newMemoFile];
                                filaS3Upload{!floorplanMemoPhotoGD901001Parameter.photosubType}(fileList);
                            }
                        );

                    });
                });
            }

            //描画開始イベント
            function drawStart(event) {
                if (!isDrawMode) {
                    return;
                }
                event.preventDefault();
                isDrawing = true;
                prevX = currX;
                prevY = currY;
                if (supportTouch) {
                    currX = event.changedTouches[0].clientX - canvas.getBoundingClientRect().left;
                    currY = event.changedTouches[0].clientY - canvas.getBoundingClientRect().top;
                } else {
                    currX = event.clientX - canvas.getBoundingClientRect().left;
                    currY = event.clientY - canvas.getBoundingClientRect().top;
                }
                context.beginPath();
                context.fillStyle = "black";
                context.fillRect(currX, currY, 2, 2);
                context.closePath();
            }
            //描画イベント
            function draw(event) {
                if (!isDrawMode) {
                    return;
                }
                event.preventDefault();
                if (isDrawing) {
                    prevX = currX;
                    prevY = currY;
                    if (supportTouch) {
                        currX = event.changedTouches[0].clientX - canvas.getBoundingClientRect().left;
                        currY = event.changedTouches[0].clientY - canvas.getBoundingClientRect().top;
                    } else {
                        currX = event.clientX - canvas.getBoundingClientRect().left;
                        currY = event.clientY - canvas.getBoundingClientRect().top;
                    }
                    context.beginPath();
                    context.moveTo(prevX, prevY);
                    context.lineTo(currX, currY);
                    context.strokeStyle = "black";
                    context.lineWidth = "2";
                    context.stroke();
                    context.closePath();
                }
            }
            //描画終了イベント
            function drawStop(event) {
                if (!isDrawMode) {
                    return;
                }
                if (isDrawing) {
                    context.stroke();
                    context.closePath();
                    isDrawing = false;
                }
            }
            //描画内容のクリア
            function clearCanvas() {
                context.clearRect(0, 0, canvas.width, canvas.height);
            }
        </script>
        <apex:outputPanel id="roomLayoutAplScript" >
            <script>           
                // 間取り図アプリを新規ウィンドウで開く処理
                var popup;      // ウィンドウ
                async function isFloorplanPhotoApliOpen() {
                    var form;
                    var apiKeyPost;
                    var roomlayoutFilepathPost;
                    var autoAnalysisFlgPost;
                    var fqdn;
                    try{
                        var apiKey = '{!roomLayoutApiKey}';			// 間取り図APIキー
                        var madoriUrl = '{!roomLayoutApiUrl}';      // 間取り図アプリURL
                        if(!apiKey || !madoriUrl){
