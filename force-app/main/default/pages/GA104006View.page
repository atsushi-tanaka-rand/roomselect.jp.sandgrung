        <script>
            // 所在階変更時の設備「1階」「2階以上」「最上階」設定
            function setConditionIsFloorAny() {
                var floorValue = document.getElementById("{!$Component.floor}").value;
                var floorType = document.getElementById("{!$Component.floorType}");
                var floorTypeOptions = floorType.querySelectorAll('option');
                var floorTypeValue = '';
                for (var i=0; i < floorTypeOptions.length; i++) {
                    if (floorTypeOptions[i].selected) {
                        floorTypeValue = floorTypeOptions[i].value;
                    }
                }
                var isFirstFloorChecked = false;
                var isSecondFloorOverChecked = false;
                var isTopFloorChecked = false;
                if (floorTypeValue == "{!FLOORTYPE_GROUND}") {
                    if (floorValue == '1') {
                        isFirstFloorChecked = true;
                    }
                    if (parseInt(floorValue) >= 2) {
                        isSecondFloorOverChecked = true;
                    }
                    if (floorValue == "{!screenPropertyRecord.groundFloorCount}") {
                        isTopFloorChecked = true;
                    }
                }
                setCheckBoxChecked('isFirstFloor'     , isFirstFloorChecked);
                setCheckBoxChecked('isFirstFloor2'     , isFirstFloorChecked);
                setCheckBoxChecked('isSecondFloorOver', isSecondFloorOverChecked);
                setCheckBoxChecked('isSecondFloorOver2', isSecondFloorOverChecked);
                setCheckBoxChecked('isTopFloor'       , isTopFloorChecked);
                setCheckBoxChecked('isTopFloor2'       , isTopFloorChecked);
            }
            // バルコニー方向変更時の設備「南向き」設定
            function setConditionIsSouth(input) {
                var checked = false;
                if (input.value == "{!BALCONYDIRECTION_SOUTHEAST}"
                    || input.value == "{!BALCONYDIRECTION_SOUTH}"
                    || input.value == "{!BALCONYDIRECTION_SOUTHWEST}") {
                    checked = true;
                }
                setCheckBoxChecked('isSouth', checked);
                setCheckBoxChecked('isSouth2', checked);
            }
            // 部屋一覧未選択チェック
            async function checkRoomSelected() {
                var roomListBlock = getJQueryObjectByApexTagId('roomListBlock');
                var checkList = roomListBlock.find('input[id$="roomCheck"]');
                var isChecked = false;
                for (var i=0; i < checkList.length; i++) {
                    if (checkList[i].checked) {
                        isChecked = true;
                    }
                }
                if (isChecked) {
                    checkDelete('deleteRoom');
                } else {
                    await showAlert("{!MSG_COM_W_SELECTION_OR_MORE_DELETE_ROOM}");
                }
            }
            // 引渡種別変更時の設備「即入居可」設定
            function setConditionIsTurnkey(input) {
                var checked = false;
                if (input.value == "{!DELIVERYTYPE_IMMEDIATE}") {
                    checked = true;
                }
                setCheckBoxChecked('isTurnkey', checked);
                setCheckBoxChecked('isTurnkey2', checked);
            }
            // 定期借家区分変更時の設備「定期借家を含まない」設定
            function setConditionIsNotFixedTermTenant(input) {
                var checked = false;
