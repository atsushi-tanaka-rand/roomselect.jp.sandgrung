            // メモ入力欄の表示処理
            function dispFloorplanMemoBlock() {
                // 表示中の間取り図（メモありorメモなし）をメモ入力欄の間取り図に表示
                var floorplanPhoto;
                if (isMemoDisp) {
                    floorplanPhoto = document.getElementById("{!$Component.floorplanPhotoMemo}");
