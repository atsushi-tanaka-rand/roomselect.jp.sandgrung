<!-- 物件登録画面 -->
<apex:page controller="GA104006Controller" docType="html-5.0" standardStylesheets="false" title="物件登録">
    <apex:form id="form">

    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta name="format-detection" content="telephone=no" />
        <meta name="robots" content="noindex" />
        <title>物件登録</title>
        <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/reset.min.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/foundation.css')}" />
        <apex:stylesheet value="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" />
        <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/modaal.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/style.css')}" />
        <apex:stylesheet value="{!URLFOR($Resource.external_file, 'external_file/common_style.css')}" />
        <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jquery-3.6.0.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jquery.blockUI.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.designer_file, 'designer_file/js/modaal.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.external_file,'external_file/common_screen_util.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.external_file,'external_file/input_rules.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jQueryUI/jquery-ui.min.js')}" />
        <apex:stylesheet value="{!URLFOR($Resource.jQuery,'jQuery/jQueryUI/jquery-ui.css')}" />
        <!-- <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jQueryUI/external/jquery/jquery.js')}"/> -->
    </head>

    <style type="text/css">
        /* 添付ファイル名表示用 */
        .attachmentfile {
            text-decoration: none;  /* 下線なし */
            display: inline;
            margin-right: 10px;
        }
        .attachmentfile:link {
            color: #303030;
        }
        .attachmentfile:visited {
            color: #303030;
        }
        .attachmentfile:hover {
            color: #303030;
        }
        .attachmentfile:active {
            color: #303030;
        }
        /* ボックス表示選択リスト apex:inputCheckbox用の設定 */
        input[type=checkbox].choice_box {
            display: none; /* チェックの非表示 */
        }
        input[type=checkbox].choice_box:checked + label {
            background: #d2d2d2; /* 選択時の背景色 */
        }
        /* ボックス表示選択リストの動的横幅設定 apex:inputCheckbox用の設定 */
        input[type=checkbox].choice_box_wide + label {
            display: inline-block;
            white-space: nowrap;
            padding-right: 10px;
            padding-left: 10px;
            height: 25px;
            line-height: 25px;
            font-size:1.2rem;
            cursor: pointer;
            border: 1px solid #d2d2d2;
        }
        /* ボックス表示選択リストの折り返し設定 apex:selectRadio用の設定 */
        table.choice_box_wrap {
            display: block;
            text-align: left;
        }
        table.choice_box_wrap tbody {
            float: left;
            width: 120px;
        }
        table.choice_box_wrap tr {
            display: inline;
            font-size: 0px;
            width: 120px;
        }
        table.choice_box_wrap td {
            display: inline-block;
            width: 57px;
        }
        .longtext_output_only {
            border-top: 0px;
            border-left: 0px;
            border-right: 0px;
            border-radius: 0px;
            height: auto;
            min-height: 35px;
            margin-bottom: 0px;
        }
        .height-auto {
            height: auto;
        }
        input[type=checkbox].togglebtn {
            /* チェックの非表示 */
            display: none;
        }
        input[type=checkbox].togglebtn:checked + label {
            /* ON時の文字・背景色 */
            background-color: #355eb0;
            color: #ffffff;
        }
        .togglebtn:disabled + label, .checkButton:disabled + label {
            background-position: 0 -90px;
            border-color: #c4c4c4;
            color: #909090;
            border: 1px solid #b5b5b5;
            background: #e8e8e9 url(/img/alohaSkin/btn_sprite.png) repeat-x right top;
            border-radius: 3px;
            cursor: default;
        }
        .img-wrapper_wide:before {
            padding-top: 42%;
        }
        .criticalItem {
            color:#FF0000;
        }
        .autoMaintenanceDifferenceChangeItem {
            border: 1px solid #FF0000;
        }
        .autoMaintenanceDifferenceChangeItemLabel {
            border-bottom: 1px solid  #FF0000;
        }
        .parent-row .serach_check5 {
            visibility: hidden;
        }
        .lborder-hidden{
            border-left: hidden !important;
        }
    </style>

    <body id="ga104006">

        <main>
            <article>

                <section class="post-pop">
                    <apex:pageMessages id="messageArea" />
                    <section style="display:flex">
                        <h1 class="fontb font2rem">物件登録</h1>
                        <apex:commandButton style=" visibility:hidden;" rerender="dummy"/> 
                    </section>
                    <section>
                        <apex:outputPanel id="allTabBlock" layout="block" styleClass="border-g">
                            <apex:inputHidden id="propertyId" value="{!screenPropertyRecord.Id}" />
                            <!-- ヘッダ -->
                            <div class="flex-wrap justify-between back_graydark pdr30 pdl30 pdt10px pdb10px pdr10_xs pdl10_xs ">
                                <h2 class="fontb font18">
                                    <apex:outputText value="{! screenPropertyRecord.titleName}" />
                                </h2>
                                <apex:outputPanel id="haederButtonBlock" layout="block" styleClass="flex-wrap">
                                    <!--▼問合せ291で変更-->
                                    <apex:commandButton value="　資料　" styleClass="btn-w-2 btn-w-2-r"
                                                        disabled="{!OR(isOpenError, isCopy, isNew)}"
                                                        rerender="dummy"
                                                        onclick=" showPage('GA104004View', '', 'TRUE', 'Id={!propertyId}')" />
                                    <apex:commandButton value="　MAP　" styleClass="btn-w-2 btn-w-2-r"
                                                        disabled="{!OR(isOpenError, isCopy, isNew)}"
                                                        action="{!createPropertyMapParameter}"
                                                        rerender="dummy"
                                                        oncomplete="showPage('GA104002View', '', 'TRUE', '{!GA104002Parameter}')"  />      
                                    <!--▲問合せ291で変更-->                                  
                                    <apex:commandButton value="　物件コピー　" styleClass="btn-w-2 btn-w-2-r"
                                                        disabled="{!OR(isOpenError, isCopy, isNew)}"
                                                        onclick="blockUi();"
                                                        action="{!clickPropertyCopyButton}"
                                                        oncomplete="unblockUi();"
                                                        reRender="allTabBlock, accordionScript, footerButtonBlock,recoProSection" />
                                    <apex:commandButton value="　名寄せ　" styleClass="btn-w-2"
                                                        disabled="{!OR(isOpenError, isCopy, isNew, screenPropertyRecord.IsMergeCompletedFlag == true)}"
                                                        onclick=""
                                                        oncomplete="showPage('GD601006View', '', 'true', 'Id={!buildingId}&Mode=物件名寄せ')"
                                                        reRender="messageArea" />
                                    <apex:commandButton value="　削除　" styleClass="btn-w-2 btn-w-2-1"
                                                        rendered="{!AND(!isOpenError, !isCopy, !isNew)}"
                                                        onclick="checkDelete('delete')"
                                                        reRender="dummy" />
                                    <!-- 削除ボタン処理 -->
                                    <apex:actionFunction name="clickDeleteButton" action="{!clickDeleteButton}"
                                                        oncomplete="unblockUi();if(!{! hasErrorMessage}){alertDeleteFinish()}else{gotoTop()}"
                                                        rerender="messageArea, allTabBlock, accordionScript" />
                                </apex:outputPanel>
                            </div>
                            <div class="back_graylite  pdr30 pdl30 pdt10px pdb10px pdr10_xs pdl10_xs flex-wrap flex_wrap justify-between">
                                <div class="flex-wrap flex_wrap justify-between wd55 wd100_md mb10">
                                    <div class="wd20">
                                        <div>物件コード</div>
                                        <apex:outputtext value="{!screenPropertyRecord.headerPropertyCode}" />
                                    </div>
                                    <div class="wd20 left-border pdl7 ">
                                        <div>総賃料</div>
                                        <apex:outputtext value="{0, number, ###,###}円">
                                            <apex:param value="{!screenPropertyRecord.headerAllRentFee}" />
                                        </apex:outputtext>
                                    </div>
                                    <div class="wd20 left-border pdl7">
                                        <div>賃料</div>
                                        <apex:outputtext value="{0, number, ###,###}円">
                                            <apex:param value="{!screenPropertyRecord.headerRentFee}" />
                                        </apex:outputtext>
                                    </div>
                                    <div class="wd20 left-border pdl7">
                                        <div>管理費</div>
                                        <apex:outputtext value="{0, number, ###,###}円">
                                            <apex:param value="{!screenPropertyRecord.headerManagementFee}" />
                                        </apex:outputtext>
                                    </div>
                                    <div class="wd20 left-border pdl7">
                                        <div>敷金/保証金</div>
                                        <apex:outputtext value="{0, number, ###,###}円">
                                            <apex:param value="{!screenPropertyRecord.headerSecurityDepositYen}" />
                                        </apex:outputtext>
                                        /
                                        <apex:outputtext value="{0, number, ###,###}円">
                                            <apex:param value="{!screenPropertyRecord.headerDepositYen}" />
                                        </apex:outputtext>
                                    </div>
                                </div>
                                <div class="flex-wrap flex_wrap justify-between wd44 wd100_md mb10">
                                    <div class="wd25 left-border pdl7 pdleft0_xs border-left-none_xs wd20_md">
                                        <div>礼金</div>
                                        <apex:outputtext value="{0, number, ###,###}円">
                                            <apex:param value="{!screenPropertyRecord.headerKeyMoneyYen}" />
                                        </apex:outputtext>
                                    </div>
                                    <div class="wd25 left-border pdl7 wd20_md">
                                        <div>間取り/専有面積</div>
                                        <apex:outputtext value="{!screenPropertyRecord.headerRoomLayout}" />
                                        /
                                        <apex:outputtext value="{0, number, ###.##}㎡">
                                            <apex:param value="{!screenPropertyRecord.headerRoomArea}" />
                                        </apex:outputtext>
                                    </div>
                                    <div class="wd25 left-border pdl7 wd40_md">
                                        <div>掲載</div>
                                        <apex:outputtext value="{!screenPropertyRecord.headerPosting}" />
                                    </div>
                                    <div class="wd25 left-border pdl7 wd20_md">
                                        <div>スーモスコア参考値</div>
                                        <apex:outputtext id="suumoScore" value="{!screenPropertyRecord.headerScore}点" />
                                    </div>
                                </div>
                            </div>

                            <apex:outputPanel id="mergeAlertMessage" layout="block">
                                <apex:outputlabel value="【Attention】 部屋番号が一致する物件情報が存在します。 「名寄せ」 を実施してください。"
                                                style="color:red;"
                                                rendered="{!AND(screenPropertyRecord.IsMergeCompletedFlag == false, propertyId != '', propertyId != null, !isCopy, !isOpenError)}"><br/></apex:outputlabel>
                                <apex:outputlabel value="【Attention】 元付業者と業者名・電話番号・住所のいずれか、または全てが一致する業者情報が存在します。元付タブ元付業者より 「業者名寄せ」 を実施してください。"
                                                style="color:red;"
                                                rendered="{!AND(screenPropertyRecord.FormerSupplierIsMergeCompletedFlag == false,screenPropertyRecord.FormerSupplierId != '',screenPropertyRecord.FormerSupplierId != null)}"><br/></apex:outputlabel>
                                <apex:outputlabel value="【Attention】 貸主と業者名・電話番号・住所のいずれか、または全てが一致する業者情報が存在します。元付タブ貸主より 「業者名寄せ」 を実施してください。"
                                                style="color:red;"
                                                rendered="{!AND(screenPropertyRecord.CreditorIsMergeCompletedFlag == false,screenPropertyRecord.CreditorId != '',screenPropertyRecord.CreditorId != null)}"><br/></apex:outputlabel>
                                <apex:outputlabel value="【Attention】 所有者と業者名・電話番号・住所のいずれか、または全てが一致する業者情報が存在します。元付タブ所有者より 「業者名寄せ」 を実施してください。"
                                                style="color:red;"
                                                rendered="{!AND(screenPropertyRecord.OwnerIsMergeCompletedFlag == false,screenPropertyRecord.OwnerId != '',screenPropertyRecord.OwnerId != null)}"><br/></apex:outputlabel>
                            </apex:outputPanel>

                            <div class="tab_wrap">
                                <input id="tab1" type="radio" name="tab_btn" checked="checked" />
                                <input id="tab2" type="radio" name="tab_btn" />
                                <input id="tab3" type="radio" name="tab_btn" />
                                <input id="tab4" type="radio" name="tab_btn" />
                                <input id="tab5" type="radio" name="tab_btn" />
                                <input id="tab6" type="radio" name="tab_btn" />
                                <input id="tab7" type="radio" name="tab_btn" />
                                <div class="tab_area flex-wrap justify-between ">
                                    <label class="tab1_label" for="tab1">物件</label>
                                    <label class="tab2_label" for="tab2">料金</label>
                                    <label class="tab3_label" for="tab3">設備</label>
                                    <label class="tab4_label" for="tab4">画像</label>
                                    <label class="tab5_label" for="tab5">間取り図</label>
                                    <label class="tab6_label" for="tab6">元付</label>
                                    <label class="tab7_label" for="tab7">公開</label>
                                </div>

                                <div class="panel_area">

                                    <!-- 物件タブ -->
                                    <div id="panel1" class="tab_panel">
                                        <apex:outputLabel value="※印が付いた対象項目が変更されると、自動メンテナンスが起動します。"><br/></apex:outputLabel>
                                        <apex:outputLabel value="赤枠部分が、自動メンテナンス起動に伴い、条件変更される対象項目です。"></apex:outputLabel>
                                        <ul id="accordion" class="accordion-defalt">
                                            <!-- 基本情報 -->
                                            <li class="default open">
                                                <div class="link">
                                                    <i class="fa fa-chevron-up"></i>基本情報</div>
                                                <ul class="submenu-defalt">
                                                    <li>
                                                        <div class="flex-wrap flex_wrap justify-between align_center ">
                                                            <!-- <div class=" wd32_8 wd100_xs"> -->
                                                            <div class=" mb10 wd32_8 wd49_md wd100_xs">
                                                                <div class="information_title">
                                                                    <apex:outputText value="建物"
                                                                                    rendered="{!IF(screenPropertyRecord.buildingId == null, true, false)}" />
                                                                    <apex:commandLink value="建物" styleClass="blue"
                                                                                    rendered="{!IF(screenPropertyRecord.buildingId == null, false, true)}"
                                                                                    oncomplete="unblockUi();showPage('GA104003View', '', 'true', 'Id={!screenPropertyRecord.buildingId}')"
                                                                                    reRender="dummy" />
                                                                </div>
                                                                <div class="information_contents" style="margin-bottom: 0px;">
                                                                    <apex:outputtext value="{!screenPropertyRecord.buildingName}"/>
                                                                </div>
                                                            </div>
                                                            <div class="mb10 wd32_8 wd100_xs">
                                                                <div class="information_title">部屋番号</div>
                                                                <div>
                                                                    <apex:input type="text" value="{!screenPropertyRecord.Name}" styleClass="form-control"
                                                                                onchange="correctInputRules(this,'text',80)" />
                                                                </div>
                                                            </div>
                                                            <div class="mb10 wd32_8 wd100_xs flex-wrap justify-between">
                                                                <div class="wd48 wd49_xs ">
                                                                    <div class="information_title">所在階</div>
                                                                    <div>
                                                                        <apex:input id="floor" type="text" value="{!screenPropertyRecord.Floor}" styleClass="form-control"
                                                                                    onchange="correctInputRules(this,'number',3,0);setConditionIsFloorAny()" />
                                                                    </div>
                                                                </div>
                                                                <div class="wd48 wd49_xs ">
                                                                    <div class="information_title">地下階区分</div>
                                                                    <div>
                                                                        <!-- 選択リスト -->
                                                                        <apex:selectList id="floorType" styleClass="form-control" size="1" value="{!screenPropertyRecord.FloorType}"
                                                                                        onchange="setConditionIsFloorAny()">
                                                                            <apex:selectOptions value="{!selectFloorTypeList}" />
                                                                        </apex:selectList>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="mb10 wd16 wd49_xs ">
                                                                <div class="information_title">部屋数</div>
                                                                <div class=" ">
                                                                    <apex:input type="text" value="{!screenPropertyRecord.RoomCount}" styleClass="form-control"
                                                                                onchange="correctInputRules(this,'number',2,0)" />
                                                                </div>
                                                            </div>
                                                            <div class=" mb10 wd16 wd49_xs ">
                                                                <div class="information_title">間取り</div>
                                                                <div>
                                                                    <!-- 選択リスト -->
                                                                    <apex:selectList styleClass="form-control" size="1" value="{!screenPropertyRecord.RoomLayout}">
                                                                        <apex:selectOptions value="{!selectRoomLayoutList}" />
                                                                    </apex:selectList>
                                                                </div>
                                                            </div>
                                                            <div class="mb10 wd16 wd49_xs ">
                                                                <div class="information_title">専有面積</div>
                                                                <div class=" ">
                                                                    <apex:input type="text" value="{!screenPropertyRecord.RoomArea}" styleClass="form-control"
                                                                                onchange="correctInputRules(this,'number',3,2)" />
                                                                </div>
                                                            </div>
                                                            <div class=" mb10 wd16 wd49_xs ">
                                                                <div class="information_title">バルコニー方向</div>
                                                                <div>
                                                                    <!-- 選択リスト -->
                                                                    <apex:selectList styleClass="form-control" size="1"
                                                                                    value="{!screenPropertyRecord.BalconyDirection}"
                                                                                    onchange="setConditionIsSouth(this)">
                                                                        <apex:selectOptions value="{!selectBalconyDirectionList}" />
                                                                    </apex:selectList>
                                                                </div>
                                                            </div>
                                                            <div class="mb10 wd16 wd49_xs ">
                                                                <div class="information_title">バルコニー面積</div>
                                                                <div class=" ">
                                                                    <apex:input type="text" value="{!screenPropertyRecord.BalconyArea}" styleClass="form-control"
                                                                                onchange="correctInputRules(this,'number',3,2)" />
                                                                </div>
                                                            </div>
                                                            <div class=" mb10 wd16 wd49_xs "></div>
                                                            <div class=" mb10 wd16 wd49_xs "></div>
                                                            <div class=" mb10 wd16  wd49_xs "></div>
                                                            <div class=" mb10 wd100 wd100_xs">
                                                                <div class="information_title">自社物フラグ</div>
                                                                <div>
                                                                    <apex:inputCheckbox id="isOwnCompanyPropertyFlag" value="{!screenPropertyRecord.IsOwnCompanyPropertyFlag}" />
                                                                    <apex:outputLabel for="isOwnCompanyPropertyFlag" styleClass="serach_check" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </li>
                                                </ul>
                                            </li>
                                            <!-- 部屋詳細 -->
                                            <li class="default open">
                                                <div class="link">
                                                    <i class="fa fa-chevron-up"></i>部屋詳細</div>
                                                <ul class="submenu-defalt">
                                                    <li>
                                                        <form name="stationform">
                                                            <div class="text-right mb20">
                                                                <apex:commandButton value="新規" styleClass="modal btn btn btn-sm btn-normal"
                                                                                    disabled="{!isOpenError}"
                                                                                    onclick="blockUi();"
                                                                                    action="{!openRoomModalCreate}"
                                                                                    oncomplete="unblockUi();jQuery('#dialog-modal1').dialog('open');"
                                                                                    reRender="GA104006Modal1Form" />
                                                                <apex:commandButton value="削除" styleClass="btn btn btn-sm btn-normal"
                                                                                    disabled="{!isOpenError}"
                                                                                    onclick="checkRoomSelected()"
                                                                                    reRender="dummy" />
                                                            </div>
                                                            <apex:outputPanel id="roomListBlock" layout="block">
                                                            <table class="gray mb30" style="height:302px;">
                                                                <tbody>
                                                                    <tr>
                                                                        <th class="wd0 "></th>
                                                                        <th class="wd3 "></th>
                                                                        <th class="wd3">部屋帖数</th>
                                                                        <th class="wdlast">部屋タイプ</th>
                                                                    </tr>
                                                                    <!-- 部屋レコード START -->
                                                                    <apex:variable var="roomIndex" value="0" />
                                                                    <apex:repeat value="{!screenPropertyRecord.ScreenRoomList}" var="roomRecord">
                                                                        <tr>
                                                                            <td>
                                                                                <apex:outputText value="{!VALUE(roomIndex)+1}" />
                                                                            </td>
                                                                            <td>
                                                                                <apex:inputCheckbox id="roomCheck" value="{!roomRecord.IsChecked}" />
                                                                                <apex:outputLabel for="roomCheck" styleClass="serach_check3" />&nbsp;&nbsp;
                                                                                <apex:actionRegion >
                                                                                    <apex:commandButton value="　編集　" styleClass="btn-w-2"
                                                                                                    disabled="{!isOpenError}"
                                                                                                    onclick="blockUi();"
                                                                                                    action="{!openRoomModalUpdate}"
                                                                                                    oncomplete="unblockUi();jQuery('#dialog-modal1').dialog('open');"
                                                                                                    reRender="GA104006Modal1Form">
                                                                                        <apex:param name="roomIndex" value="{!VALUE(roomIndex)}" assignTo="{!selectedRoomIndex}" />
                                                                                    </apex:commandButton>
                                                                                </apex:actionRegion>
                                                                            </td>
                                                                            <td>
                                                                                <apex:outputText value="{!roomRecord.Room.RoomFloorSpace__c}" />
                                                                            </td>
                                                                            <td>
                                                                                <apex:outputText value="{!roomRecord.Room.RoomType__c}" />
                                                                            </td>
                                                                        </tr>
                                                                        <apex:variable var="roomIndex" value="{!VALUE(roomIndex)+1}" />
                                                                    </apex:repeat>
                                                                    <!-- 部屋レコード   END -->
                                                                </tbody>
                                                            </table>
                                                            </apex:outputPanel>
                                                        </form>
                                                    </li>
                                                </ul>
                                            </li>
                                            <!-- 引渡情報 -->
                                            <li class="default open">
                                                <div class="link">
                                                    <i class="fa fa-chevron-up"></i>引渡情報</div>
                                                <ul class="submenu-defalt">
                                                    <li>
                                                        <div class="flex-wrap  flex_wrap justify-between align_center ">
                                                            <div class=" mb10 wd16 wd49_xs ">
                                                                <div class="information_title">現況※</div>
                                                                <div>
                                                                    <!-- 選択リスト -->
                                                                    <apex:selectList styleClass="form-control" size="1" value="{!screenPropertyRecord.LiveStatus}">
                                                                        <apex:selectOptions value="{!selectLiveStatusList}" />
                                                                    </apex:selectList>
                                                                </div>
                                                            </div>
                                                            <div class=" mb10 wd16 wd49_xs">
                                                                <div class="information_title">引渡種別</div>
                                                                <div>
                                                                    <!-- 選択リスト -->
                                                                    <apex:selectList styleClass="form-control" size="1" value="{!screenPropertyRecord.DeliveryType}"
                                                                                    onchange="setConditionIsTurnkey(this)">
                                                                        <apex:selectOptions value="{!selectDeliveryTypeList}" />
                                                                    </apex:selectList>
                                                                </div>
                                                            </div>
                                                            <div class=" mb10 wd16 wd49_xs">
                                                                <div class="information_title">引渡年</div>
                                                                <div>
                                                                    <apex:input type="text" value="{!screenPropertyRecord.DeliveryYear}" styleClass="form-control"
                                                                                onchange="correctInputRules(this,'number','4','','4000',false,'1700')" />
                                                                </div>
