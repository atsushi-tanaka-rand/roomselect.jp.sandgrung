            // 特定優良賃貸住宅セクション 活性/非活性制御
            function setSpecialRentEnabled() {
                var checkInput = document.getElementById("{!$Component.specialRentCheck}");
                var inputElements = [
                    document.getElementById("{!$Component.specificExcellentRentBurdenUpperLimit}")
                    ,document.getElementById("{!$Component.specificExcellentRentBurdenLowerLimit}")
                    ,document.getElementById("{!$Component.specificExcellentRentFeeFluctuationType}")
                    ,document.getElementById("{!$Component.specificExcellentRentFeeUpwardRate}")
                    ,document.getElementById("{!$Component.specificExcellentRentSupportYears}")
                    ,document.getElementById("{!$Component.specificExcellentRentRemarks}")
                ];
                setEnabledFromChecked(checkInput.checked, inputElements);
            }
            // 添付ファイルの追加処理
            async function addAttachment(inputFileObject){
                var reader = new FileReader();
                var file = inputFileObject.files[0];
                var parentId;
                var fileName;
                var fileBody;
                var fileContent;
                var fileSize = 0;
                var totalFileSize = 0;
                // ファイル選択なしの場合、処理終了
                if (typeof file == "undefined") {
                    return;
                }
                // ファイルの読み込み処理を作成する
                reader.onload = async function(event) {
                    // 物件Id取得
                    parentId = document.getElementById("{!$Component.propertyId}").value;
                    // ファイル名取得
                    fileName = file.name;
                    // ファイル内容取得
                    fileBody = event.target.result;
                    fileContent = String(fileBody);
                    fileContent = fileContent.substr(fileContent.indexOf(',') + 1);
                    // 登録済ファイルサイズ取得
                    totalFileSize = Number(document.getElementById("{!$Component.attachmentFileSizeTotal}").value);
                    // ファイルサイズ取得
                    fileSize = file.size;
                    // 5MB（5,242,880Byte）超過チェック
                    totalFileSize = totalFileSize + fileSize;
                    if (totalFileSize > 5242880) {
                        await showAlert('合計で5BMを超えるファイルの登録はできません。');
                        return;
                    }
                    blockUi();
                    // 添付ファイルを登録（JavascriptでAttachmentオブジェクトへ登録）
                    uploadAttachment(parentId, fileName, fileContent, fileSize);
                    // 添付ファイルの読み直し（actionFunction）
                    reloadAttachment();
                }
                // ファイルの読み込みを実行
                reader.readAsDataURL(file);
            }
            function uploadAttachment(parentid, filename, filecontent, filesize) {
                var att = new sforce.SObject('Attachment');
                att.ParentId    = parentid;
