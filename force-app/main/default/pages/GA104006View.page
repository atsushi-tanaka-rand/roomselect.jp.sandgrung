                }
                return;
            }
            //メモ入力トグルボタン（実態はチェックボックス）により描画モード（true:on/false:off）を設定
            function changeFloorplanPhotoDrawMode(obj) {
                isDrawMode = obj.checked;
                isDrawing = false;
                if (isDrawMode) {
                    //メモ表示/非表示ボタンを非活性化
                    document.getElementById("{!$Component.IsFloorplanPhotoMemoDisp}").disabled = true;
                    //メモ入力欄を表示
                    document.getElementById("{!$Component.floorplanPhotoMemoButtonBlock}").style.display = '';
