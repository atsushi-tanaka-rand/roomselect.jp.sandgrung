            //メモ表示/非表示ボタン 画像切替処理
            function changeFloorplanPhotoMemoDisp(obj) {
                isMemoDisp = obj.checked;
                if (isMemoDisp) {
                    document.getElementById("{!$Component.floorplanPhotoImageBlock}").style.display = 'none';
