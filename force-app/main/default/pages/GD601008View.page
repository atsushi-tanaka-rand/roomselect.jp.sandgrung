        $(function() {
            
            var Accordion = function(el, multiple) {
                
                this.el = el || {};
                
                this.multiple = multiple || false;
                
                
                
                // Variables privadas
                
                var links = this.el.find('.link');
                
                // Evento
                
                links.on('click', {el: this.el, multiple: this.multiple}, this.dropdown)
                
            }
            
            
            
            Accordion.prototype.dropdown = function(e) {
                
                var eventClass = event.target.className;
                
                if(eventClass != "modal btn-w-2 btn-w-2_normal blue"){
                    
                    var $el = e.data.el;
                    
                    $this = $(this),
                        
                        $next = $this.next();
                    
                    
                    
                    $next.slideToggle();
                    
                    $this.parent().toggleClass('open');
                    
                    
                    
                    if (!e.data.multiple) {
                        
                        // $el.find('.submenu-defalt').not($next).slideUp().parent().removeClass('open');
                        
                    };
                    
                }
                
                
                
            }
            
            
            
            var accordion = new Accordion($('#accordion'), false);
            
        });
        
        
        
        
        
        
        
        </script>
        <script type="text/javascript">
          //名寄せ確認メッセージ
          async function checkMerge(){
              var message = "{! MSG_COM_V_EXECUTE_MERGE}";
              var retVal = await showConfirm(message);
              if (retVal) {
                  blockUi();
                  save();
              }          
          }

         //名寄せ完了メッセージ
          async function alertMergeFinish(targetId,webPublishingNoMessageFlg,IsDeleteRecommendPropertyMessageFlg){
            // 名寄せによるweb公開をしないと変更となった場合のメッセージを表示させる判定
            if (webPublishingNoMessageFlg === 'true'){
                // web公開しないに処理で変更した場合のメッセージ
                await showAlert("{! MSG_PROP_I_NOTICE_WEBPUBLISHING_NO}");
            }
            if(IsDeleteRecommendPropertyMessageFlg === 'true') {
                // おすすめ物件を削除した場合のメッセージ
                await showAlert("{! MSG_PROP_I_NOTICE_DELETE_RECOMMEND}");
            }

            //完了メッセージ
            await showAlert("{! MSG_COM_I_MERGE_END}");
            
            //建物名寄せ画面（物件名寄せモード）に遷移
            showPage('GD601006View', '_self', 'true', 'Id=' + targetId + '&Mode=物件名寄せ');
        } 
          </script>
        
        
