                function getJQueryObjectByApexTagId(Id) {
                        return $('[id$=":'+Id+'"]');
                }

                jQuery(function () {
                    // モーダル幅の設定用
                    var clientWidth = 0; 
                    if(document.body.clientWidth > 800){
                        clientWidth = 800;
                    }else{
                        clientWidth = document.body.clientWidth;
                    }
                    //ダイアログを表示（担当者設定）
                    jQuery("#dialog-modal1").dialog({
                        modal: true,
                        autoOpen: false,
                        width: clientWidth,
                        close : function(ev, ui) {
