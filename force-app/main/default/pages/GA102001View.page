<!-- お客様情報詳細 -->
<apex:page docType="html-5.0" standardStylesheets="false" controller="GA102001Controller" title="お客様情報詳細">
    <apex:form id="GA102001" html-novalidate="novalidate">

        <head>
            <title>お客様情報詳細</title>
            <meta charset="utf-8" />
            <meta name="viewport" content="width=device-width, initial-scale=1.0" />
            <meta name="format-detection" content="telephone=no" />
            <meta name="robots" content="noindex" />
            <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/reset.min.css')}" />
            <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/foundation.css')}" />
            <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/modaal.css')}" />
            <apex:stylesheet value="{!URLFOR($Resource.designer_file,'designer_file/css/style.css')}" />
            <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jquery-3.6.0.min.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jquery.autoKana.js')}"/>
            <apex:includeScript value="{!URLFOR($Resource.designer_file, 'designer_file/js/modaal.js')}" />
            <apex:stylesheet value="https://use.fontawesome.com/releases/v5.0.6/css/all.css" />

            <apex:stylesheet value="{!URLFOR($Resource.external_file, 'external_file/common_style.css')}" />
            <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jquery.blockUI.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.external_file,'external_file/common_screen_util.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.external_file,'external_file/input_rules.js')}" />
            <apex:includeScript value="{!URLFOR($Resource.jQuery,'jQuery/jQueryUI/jquery-ui.min.js')}" />
            <apex:stylesheet value="{!URLFOR($Resource.jQuery,'jQuery/jQueryUI/jquery-ui.css')}" />

            <!-- Zip圧縮用 -->
            <apex:includeScript value="{!URLFOR($Resource.jQuery, 'jQuery/JSZip/dist/jszip.min.js')}"/>


            
        </head>

        <!-- ｃｓｓ -->
        <style type="text/css">
            #table_detail .hidden_row{
                display:none;
            }
            .site-header2{
                background-color:#ffffff;
                margin: 0 auto;
                padding: 20px 0px 0;
                position: sticky;/*固定する*/
                position: -webkit-sticky;/*Safari用ベンダープレフィックス*/
                /* top: 0;ブラウザの上からの距離はゼロ */
                width: 100%;
                /* z-index:1000; */
            }
            .serach_check_GA102001 {
                font-size: 1.3rem;
                width: 190px;
                display: inline-block;
                padding-left: 25px;
                cursor: pointer;
                height: 30px;
                line-height: 30px;
                position: relative;
            }
            /* ▼ 2021.09.21 問合せNo.600 不要なスタイルの為、削除 */
            /* ▲ 2021.09.21 問合せNo.600 */

            a.disabled{
                pointer-events: none;
            }

            .padding0{
                padding: 0px;
            }

            .padding10{
                padding: 10px;
            }

            .detailChkBoxWidth {
                width: 30px;
            }

            /* ▼ 2021.09.21 問合せNo.600 レスポンシブ対応による追加 */
            /* セクションタイトル部（左寄せ対応） */
            .section-title {
                float: left;
            }

            /* セクション中央部（回り込みの解除） */
            .section-middle {
                clear: both; 
            }

            /* 地区エリア（横並び対応） */
            .district-div {
                display: inline-block;
            }

            /* 地区リスト（サイズ対応） */
            .district-list {
                padding-left: 12px;
                padding-right: 12px;
                height: 30px;
            }
            /* ▲ 2021.09.21 問合せNo.600 */
        </style>


        <body id="GA102001Form">
            <apex:outputPanel id="GA102001">
                <main>
                    <article>
                        <apex:outputPanel id="Header" layout="block" styleClass="{!if( HeaderLockFlg == False,'site-header2','site-header')}" >
                            <apex:pageMessages id="messageArea" />
                            
                            <div class=" pdr30 pdl30  pdr10_xs pdl10_xs ">
                                <section style="display: flex">
                                   <!-- 表示時 -->
                                    <apex:outputPanel rendered="{! !isInsertFlg}" >
                                        <h1 class="fontb font2rem mb20">お客様情報詳細</h1>
                                    </apex:outputPanel>
                                    <!-- 登録時 -->
                                    <apex:outputPanel rendered="{! isInsertFlg}" >
                                        <h1 class="fontb font2rem mb20">お客様情報登録</h1>
                                    </apex:outputPanel>
                                    <apex:commandButton style=" visibility:hidden;" rerender="dummy"/> 
                                </section>
                                

                                <!-- 表示時 -->
                                <apex:outputPanel rendered="{! !isInsertFlg}">
                                
                                    <div class="flex-wrap flex_wrap align-items_end justify-between back_graydark pdr30 pdl30 pdt10px pdb10px pdr10_xs pdl10_xs">
                                        <h2 class="fontb font18">
                                            <apex:outputText value="{!screenHeaderRecord}" />
                                        </h2>
                                        <apex:outputPanel rendered="{! !isInsertFlg}" >
                                            <!-- 顧客用 -->
                                            <apex:outputPanel rendered="{! screenAccountRecord.RecordTypeId == ACCOUNT_RECTYPE_CUSTOMER}">
                                                <apex:commandButton styleclass="btn-w-2 btn-w-2_normal blue" value="　編集　" 
                                                    onclick="blockUi();"
                                                    disabled="{!isOpenError}" 
                                                    action="{!changeMode}"
                                                    oncomplete="unblockUi();gotoTop();"
                                                    reRender="GA102001,redraw"
                                                    tabIndex="1"/>
                                            </apex:outputPanel>
                                            <!-- 業者用 -->
                                            <apex:outputPanel rendered="{! screenAccountRecord.RecordTypeId == ACCOUNT_RECTYPE_SUPPLIER}">
                                                <apex:commandButton styleclass="btn-w-2 btn-w-2_normal blue" value="　編集　" 
                                                    onclick="blockUi();" disabled="{!isOpenError}" 
                                                    oncomplete="showPage('GD101002View', '_self', 'true', 'id={!screenAccountRecord.Id}');unblockUi();"
                                                    reRender="GA102001,redraw" tabIndex="1"/>
                                            </apex:outputPanel>
                                            <!-- 保証会社用 -->
                                            <apex:outputPanel rendered="{! screenAccountRecord.RecordTypeId == ACCOUNT_RECTYPE_GUARANTEE}">
                                                <apex:commandButton styleclass="btn-w-2 btn-w-2_normal blue" value="　編集　" 
                                                    onclick="blockUi();" disabled="{!isOpenError}" 
                                                    oncomplete="showPage('GF101002View', '_self', 'true', 'id={!screenAccountRecord.Id}');unblockUi();"
                                                    reRender="GA102001,redraw" tabIndex="1"/>
                                            </apex:outputPanel>
                                            <!-- 火災保険会社用 -->
                                            <apex:outputPanel rendered="{! screenAccountRecord.RecordTypeId == ACCOUNT_RECTYPE_FIRE_INSURANCE}">
                                                <apex:commandButton styleclass="btn-w-2 btn-w-2_normal blue" value="　編集　" 
                                                    onclick="blockUi();" disabled="{!isOpenError}" 
                                                    oncomplete="showPage('GF801002View', '_self', 'true', 'id={!screenAccountRecord.Id}');unblockUi();"
                                                    reRender="GA102001,redraw" tabIndex="1"/>
                                            </apex:outputPanel>
                                            <!-- 付帯業者用 -->
                                            <apex:outputPanel rendered="{! screenAccountRecord.RecordTypeId == ACCOUNT_RECTYPE_AUXILIARY}">
                                                <apex:commandButton styleclass="btn-w-2 btn-w-2_normal blue" value="　編集　" 
                                                    onclick="blockUi();" disabled="{!isOpenError}" 
                                                    oncomplete="showPage('GF201004View', '_self', 'true', 'id={!screenAccountRecord.Id}');unblockUi();"
                                                    reRender="GA102001,redraw" tabIndex="1"/>
                                            </apex:outputPanel>


                                        </apex:outputPanel>    
                                    </div>
                                </apex:outputPanel>
                            </div>
                        </apex:outputPanel>                    

                        <apex:outputPanel id="AccountDate" styleClass="site-content ">

                            <div class="border-g-t-none  pdr30 pdl30  pdr10_xs pdl10_xs ">

                                <div>　</div>
                                <!-- 表示時 -->
                                <apex:outputPanel id="WarningMessage" rendered="{! !isInsertFlg}" >
                                    <!-- 名寄せ警告 -->
                                    <apex:outputPanel rendered="{! if(screenAccountRecord.IsMergeCompletedFlag == true, false, true)}">
                                        <!-- <div class="fontred ">[Attention]氏名・メールアドレス・電話番号のいずれか、または全てが一致するお客様情報が存在します。「名寄せ」を実施してください。</div> -->
                                        <div class="fontred "> <apex:outputLabel value="{!MSG_CUST_I_NOTICE_MERGE}"/> </div>
                                    </apex:outputPanel>   
                                        
                                    <apex:outputPanel rendered="{! EchoAggregationMessageFlg && screenCorrespondRecordList[checkCorrespondIndex].echoAggregationFlg }">
                                        <!-- <div class="fontred">[Attention]対応状況に集約を必要とする内容が含まれています。「反響集約」してください。</div> -->
                                        <div class="fontred "> <apex:outputLabel value="{!MSG_PROP_I_DUPLICATE_ECHO}"/> </div>
                                    </apex:outputPanel>
                                    
                                    <!-- ▼問合せNo341で追加 電話反響フラグ警告 -->
                                    <apex:outputPanel rendered="{! !screenAccountRecord.EchoTelephoneFlag && screenAccountRecord.Situation == '電話反響' && (checkStatus == CORRESPOND_STATUS_NOTSUPPORTED || checkStatus == '')}">
                                        <!-- <div class="fontred">[Attention]状況で電話反響が選択されています。反響担当者を設定し、電話反響を登録してください。</div> -->
                                        <div class="fontred "> <apex:outputLabel value="{!MSG_ACC_W_SITUATION_ECHO_TELEPHONE}"/> </div>
                                    </apex:outputPanel>
                                    <!-- ▲問合せNo341で追加 -->
                                    <!-- ▼問合せNo411で追加 リベロ反響フラグ、LINE反響フラグ警告 -->
                                    <apex:outputPanel rendered="{! !screenAccountRecord.EchoLineFlag && screenAccountRecord.Situation == 'LINE反響' && (checkStatus == CORRESPOND_STATUS_NOTSUPPORTED || checkStatus == '')}">
                                        <div class="fontred "> <apex:outputLabel value="{!MSG_ACC_W_SITUATION_ECHO_LINE}"/> </div>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{! !screenAccountRecord.EchoLiveroFlag && screenAccountRecord.Situation == 'Livero反響' && (checkStatus == CORRESPOND_STATUS_NOTSUPPORTED || checkStatus == '')}">
                                        <div class="fontred "> <apex:outputLabel value="{!MSG_ACC_W_SITUATION_ECHO_LIVERO}"/> </div>
                                    </apex:outputPanel>
                                    <!-- ▲問合せNo411で追加 -->
                                </apex:outputPanel>
                                <ul id="accordion" class="accordion-defalt">

                                    <!-- 表示時 -->
                                    <apex:outputPanel rendered="{! !isInsertFlg}" >                                
                                    <li class="default open">

                                        <!--  ▼ 2021.09.21 問合せNo.600 レスポンシブ対応によるレイアウト変更 -->
                                        <div class="link text-right">
                                            <div class="section-title">
                                                <i class="fa fa-chevron-up"></i>対応状況
                                            </div>
                                            <span class="section-middle"></span>

                                            <apex:commandButton id="buttonPersonnel" value="担当者確定" styleclass="btn-w-2 btn-w-2_normal blue"
                                                    disabled="{!isOpenError}" 
                                                    onclick="blockUi();" action="{!openModal1}" reRender="GA102001Modal1,messageArea,redraw"
                                                    status="loadComponent" oncomplete="unblockUi(); if({!hasErrorMessage}) {return false;}; setHeaderRelease(); jQuery('#dialog-modal1').dialog('open'); "
                                                    tabIndex="2"/>
                                        </div>
                                        <!-- ▲ 2021.09.21 問合せNo.600 -->

                                        <ul class="submenu-defalt">

                                            <!-- 対応状況 -->
                                            <apex:outputPanel id="CorrespondList" >   
                                                <table class="gray" style="max-height:380px;">
                                                    <tbody>
                                                        <tr>
                                                            <th class="wd2"></th>
                                                            <th class="wd4">ステータス</th>
                                                            <th class="wd5">イベント</th>
                                                            <th class="wd5">対応日付</th>
                                                            <th class="wd5">反響担当</th>
                                                            <th class="wd5">来店担当</th>
                                                            <th class="wd5">地区</th>
                                                            <th class="wd6">物件</th>
                                                            <th class="wd6">状況</th>
                                                        </tr>
                                                        <apex:variable var="num" value="0" />
                                                        <apex:repeat value="{!screenCorrespondRecordList}" var="Correspond">
                                                            <tr>
                                                                <td>
                                                                    <apex:inputCheckbox id="checkCorrespond"  value="{!Correspond.isSelected}" onclick="blockUi();" > 
                                                                        <apex:actionSupport event="onchange"
                                                                                action="{!setButtonList}"
                                                                                oncomplete="onchangeListCheckBoxSingleAlwaysSelect(this);unblockUi();"
                                                                                reRender="buttonChanged,js,buttonPersonnel,redraw,introMailInfoBlock,WarningMessage">
                                                                            <apex:param value="{!Correspond.Status}" name="checkStatus" assignTo="{!checkStatus}"/>
                                                                            <apex:param value="{!Correspond.Event}" name="checkEvent" assignTo="{!checkEvent}"/>
                                                                            <apex:param value="{!Correspond.Id}" name="checkCorrespondId" assignTo="{!checkCorrespondId}"/>
                                                                            <apex:param value="{!Correspond.District}" name="checkDistrict" assignTo="{!checkDistrict}"/>
                                                                            <apex:param value="{!Correspond.property}" name="checkProperty" assignTo="{!checkProperty}"/>
                                                                            <apex:param value="{!Correspond.application}" name="checkApplication" assignTo="{!checkApplication}"/>
                                                                            <apex:param value="{!Correspond.SystemModstamp}" name="checkSystemModstamp" assignTo="{!checkSystemModstamp}"/>
                                                                            
                                                                            <apex:param value="{!Correspond.echoStaffName}" name="checkEchoStaff" assignTo="{!checkEchoStaff}"/>
                                                                            <apex:param value="{!Correspond.shopStaffName}" name="checkShopStaff" assignTo="{!checkShopStaff}"/>
                                                                            <apex:param value="{!Correspond.PursuitDissolveDate}" name="checkPursuitDissolveDate" assignTo="{!checkPursuitDissolveDate}"/>
                                                                            <apex:param value="{!Correspond.buildingproperty}" name="checkBuildingproperty" assignTo="{!checkBuildingproperty}"/>

                                                                            
                                                                            <apex:param value="{!VALUE(num)}" name="checkindex" assignTo="{!checkCorrespondIndex}"/>    
                                                                        </apex:actionSupport>
                                                                    </apex:inputCheckbox>
                                                                    <apex:outputlabel styleClass="serach_check5" for="checkCorrespond" />
                                                                </td>

                                                                <td><apex:outputText value="{!Correspond.Status}" /></td>
                                                                <td><apex:outputText value="{!Correspond.Event}" /></td>
                                                                <td><apex:outputText value="{!Correspond.EventDateTime}" /></td>
                                                                <td><apex:outputText value="{!Correspond.outputEchoStaff}" id="EchoStaff" /></td>
                                                                <td><apex:outputText value="{!Correspond.outputShopStaff}" id="ShopStaff" /></td>
                                                                <td><apex:outputText value="{!Correspond.District}" /></td>
                                                                <td><apex:outputText value="{!Correspond.buildingproperty}" /></td>
                                                                <td><apex:outputText value="{!Correspond.propertyLiveStatus}" /></td>
                                                                <td style="width: 1px;padding : 0px"><apex:inputHidden id="CorrespondPropertyId" value="{!Correspond.property}"/></td>
                                                
                                                                <apex:variable var="num" value="{!VALUE(num)+1}" />
                                                            </tr>
                                                        </apex:repeat>
                                                    </tbody>
                                                </table>
                                            </apex:outputPanel>

                                            <!-- 対応各種ボタン -->
                                            <apex:outputPanel id="buttonChanged" styleClass="flex-wrap  flex_wrap justify-between" style="padding-top :30px;">
                                                <!-- ▼2021.10.08 No.307 対応で物件検索押下処理変更 -->
                                                <a class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_PROPERTY_SEARCH]]}"
                                                    style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_PROPERTY_SEARCH]]}"
                                                    onclick="blockUi();openModal9();" 
                                                    tabindex="3">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_PROPERTY_SEARCH]] + '_12.png' )}"/>
                                                    物件検索
                                                </a>
                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_MAIL]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_MAIL]]}"
                                                        onclick="blockUi();openMaile();" 
                                                        tabindex="4">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_MAIL]] + '_1.png' )}"/>
                                                    メール
                                                </a>
                                                <!-- ▼ 2021.11.30 LINE連携対応より、ボタン名を修正 -->
                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_TEL]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_TEL]]}"
                                                        onclick="blockUi();openModal2Tel();" tabindex="5">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_TEL]] + '_2.png' )}"/>
                                                    TEL/SMS/メモ
                                                </a>

                                                <a class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_LINE]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_LINE]]}"
                                                        onclick="blockUi(); openLineExclusive();" tabindex="6">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_LINE]] + '_18.png' )}"/>
                                                    LINE
                                                </a>
                                                <!-- ▲ 2021.11.30 -->

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_APPOINT_CONFIRMATION]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_APPOINT_CONFIRMATION]]}"
                                                        onclick="blockUi();openModal2Apo();" tabindex="7">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_APPOINT_CONFIRMATION]] + '_4.png' )}"/>
                                                    {! if(checkStatus == TASK_CORRESPONDTYPE_APPOINTMENT && hasEvent,'アポイント変更','アポイント確定')}
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_SCHEDULE]]}"
                                                    style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_SCHEDULE]]}"
                                                    onclick="blockUi();openModal6();" tabindex="8">
                                                    
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_SCHEDULE]] + '_5.png' )}"/>
                                                    スケジュール
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_VISIT]]}"
                                                    style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_VISIT]]}"
                                                    onclick="blockUi();openVisit();" 
                                                    tabindex="9">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_VISIT]] + '_6.png' )}"/>
                                                    来店(iPad)
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_CONSIDER_AFTER_VISITING_STORE]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_CONSIDER_AFTER_VISITING_STORE]]}"
                                                        onclick="blockUi();openModal2After();" tabindex="10">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_CONSIDER_AFTER_VISITING_STORE]] + '_6.png' )}"/>
                                                    来店後検討
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_APPLICATION]]}"
                                                    style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_APPLICATION]]}"
                                                    onclick="openApplicationCheck(1);" 
                                                    tabindex="11">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_APPLICATION]] + '_9.png' )}"/>
                                                    申込
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_APPLICATION]]}"
                                                    style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_APPLICATION]]}"
                                                    onclick="openApplicationCheck(2);" 
                                                    tabindex="12">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_APPLICATION]] + '_9.png' )}"/>
                                                    申込(iPad)
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_DEPOSIT]]}"
                                                    style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_DEPOSIT]]}"
                                                    onclick="blockUi();openDeposit();" 
                                                    tabindex="13">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_DEPOSIT]] + '_8.png' )}"/>
                                                    入金
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_CONTRACT_COMPLETED]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_CONTRACT_COMPLETED]]}"
                                                        onclick="blockUi();openModal2ContractCompleted();" tabindex="14">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_CONTRACT_COMPLETED]] + '_9.png' )}"/>
                                                    契約完了
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_HOLD_FOLLOWER]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_HOLD_FOLLOWER]]}"
                                                        onclick="blockUi();openModal2HoldFollower();" tabindex="15">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_HOLD_FOLLOWER]] + '_10.png' )}"/>
                                                    <apex:outputLabel value="{! if(checkStatus == CORRESPOND_STATUS_PURSUITHOLD ,'保留解除' , '追客保留')}" style="border: 0;"  />
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_APPOINT_CANCEL]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_APPOINT_CANCEL]]}"
                                                        onclick="blockUi();openModal2AppointCancel();" tabindex="16">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_APPOINT_CANCEL]] + '_14.png' )}"/>
                                                    アポキャンセル
                                                </a>
                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_APPLICATION_CANCEL]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_APPLICATION_CANCEL]]}"
                                                        onclick="blockUi();openModal2ApplicationCancel();" tabindex="17">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_APPLICATION_CANCEL]] + '_14.png' )}"/>
                                                    申込キャンセル
                                                </a>

                                                <a  class="wd16 menu_btn {!buttonState[buttonListSet[BUTTON_END]]}"
                                                        style="pointer-events: {!buttonDisabled[buttonListSet[BUTTON_END]]}"
                                                        onclick="blockUi();openModal2End();" tabindex="18">
                                                    <apex:image alt="" url="{!URLFOR($Resource.designer_file, 'designer_file/images/icon_' + buttonURL[buttonListSet[BUTTON_END]] + '_11.png' )}"/>
