                            }
                            closepattern = '';
                        }
                    });
                });   

                //////////////////////////////////////////////////////////////
                // お気に入り物件紐づけの入力チェック（申込ボタン）
                //////////////////////////////////////////////////////////////
                async function openApplicationCheck(openflg){
        
                    // お気に入り物件が2件以上で登録されていれば抜ける。
                    var FavoritePropertyList = getJQueryObjectByApexTagId('FavoritePropertyList');
                    var checkList = FavoritePropertyList.find('tr');
                    var count = 0;
                    var checkProperty = "{! checkProperty}";
                    var setFlg = false; // 物件が既に登録されているか
                    var FavoritePropertySetCount = 0;  // お気に入り物件のデータが既にセットされている件数
                    var FavoritePropertyCheckCount = 0; // お気に入り物件のチェックしている数
                    setHeaderRelease();
                    
                    // 担当者が対応に設定されているか。
                    if(! await checkPersonnelUnconfigured(5)){
                        // されていなければ抜ける。
                        setHeaderFixed();
                        return false;
                    }

                    // 対応一覧取得
                    var CorrespondList1 = getJQueryObjectByApexTagId('CorrespondList');
                    var CorrespondList2 = CorrespondList1.find('tr');

                    // お気に入り物件と選択されている対応の比較
                    for (var i=0; i < checkList.length; i++) {
                        var checkdate = $(checkList[i]).find('input[id$="checkFavoriteProperty"]');

                        if (checkdate.length > 0) {
                            if (checkdate[0].checked) {
                                FavoritePropertyCheckCount += 1;
                                
                                var PropertyStatus =$(checkList[i]).find('span[id$="PropertyStatus"]');
                                if (PropertyStatus[0].textContent == "{!PROPERTY_LIVESTATUS_RESIDENT}"){
                                    // 居住中を選んでいる場合はエラーにする。
                                    await showAlert("{!MSG_PROP_E_PROPERTY_LINKING_RESIDENCE}");
                                    setHeaderFixed();
                                    return false;
                                }

                                // お気に入り物件の物件ID
                                var PropertyId =$(checkList[i]).find('input[id$="PropertyId"]');

                                // 対応状況の登録物件が既に登録されているのか確認
                                for (var j=0; j < CorrespondList2.length; j++) {
                                    if(CorrespondList2.length > 0 && j > 0){
                                        var CorrespondPropertyId = $(CorrespondList2[j]).find('input[id$="CorrespondPropertyId"]');
                                        if(CorrespondPropertyId[0].value == PropertyId[0].value){
                                            // 一致していればカウント。
                                            FavoritePropertySetCount += 1;
                                            break;
                                        }   
                                    }
                                }

                                count += 1;
                                // 対象のお気に入り物件と対応の物件が一致している場合
                                if(checkProperty == PropertyId[0].value){
                                    setFlg = true;
                                }
                            }
                        }
                    }
            
                    if(setFlg && FavoritePropertyCheckCount == 1){
                        // 対応とお気に入り物件が一致していてチェックが1件だけであった場合、スルー
                    }else{
                        
                        // 物件が既に紐づいている状態の内容を空の対応に紐づけようとした場合、エラーになる
                        if(FavoritePropertyCheckCount > 0 && FavoritePropertySetCount > 0 && 
                        FavoritePropertyCheckCount <= FavoritePropertySetCount ){
                            await showAlert("{!MSG_CUST_E_SET_PROPERTY}");
                            setHeaderFixed();
                            return false; 
                        }
                    }
            

                    // 対象の取引先に物件があるか判定(物件が2件の場合は紐づけをさせる)
                    if((checkProperty && setFlg && count == 1) || (checkProperty && count == 0)){
                        // 登録あり(問題なしとして抜ける)
                        blockUi();
                        if(openflg == 1){
                            // 画面直接起動
                            openApplicatiou();
                        }else{
                            // QRのモーダル表示
                            openApplicatiouQR();
                        }
                        return true;

                    }

                    if (count > 0){
                        // 問題なし
                        var message = "{!MSG_PROP_I_PROPERTY_LINKING}";
                        var retVal = await showConfirm(message);
                        if (!retVal) {
                            // 出力させない
                            setHeaderFixed();
                            return false;
                        }
                        blockUi();
                        if(openflg == 1){
                            // 画面直接起動
                            openApplicatiou();
                        }else{
                            // QRのモーダル表示
                            openApplicatiouQR();
                        }
                        return true;
                    }

                    // 選択なし
                    await showAlert("{!MSG_COM_W_SELECTION_OR_MORE_DELETE_FAVORITEPROPERTY_APPLICATION}");
                    setHeaderFixed();
                    return false;
                } 

                </script>
