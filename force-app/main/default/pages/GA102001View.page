                    async function conditionMapKeyCheck(){
						//▼ 2022.01.21 No.307 対応で物件条件選択リスト非描画時にNullエラーとならないよう修正
						var condName = zenkakuChangeHankaku(document.getElementById('{!$Component.chageCondName}').value); //入力された物件条件名
						var tmpName = document.getElementById('{!$Component.tmpCondName}').value; //SelectOptionで選択されていたインデックスの物件条件名
						document.getElementById('{!$Component.chageCondName}').value = condName; //全角英数文字を半角に変換する。

						//物件条件選択リストが表示中の場合のみ重複判定を実施
						if(document.getElementById('{!$Component.selectConditionslist}')){
							var selectConditionslist = document.getElementById('{!$Component.selectConditionslist}').options; //SelectOptionの値
							var selectedIndex = document.getElementById('{!$Component.selectConditionslist}').options.selectedIndex; //SelectOptionで選択されていたインデックス

							// 入力された物件条件名が、選択中の物件条件名以外で、同名のものが存在する場合「ご入力いただいた物件条件名が既に存在します。」を表示させ、選択中の物件条件名を再度セットする。
							for(let i = 0; i<selectConditionslist.length; i++){
								if(selectConditionslist[i].label == condName && tmpName != condName){
									await showAlert("{! MSG_COM_I_E_NG_PROPERTY_SAMENAME}");
									document.getElementById('{!$Component.chageCondName}').value = selectConditionslist[selectedIndex].label;
									return;
								}
							}
						}
						//▲ 2022.01.21 No.307 対応で物件条件選択リスト非描画時にNullエラーとならないよう修正

                        // 未入力の場合「物件条件名が未入力です。」を表示させ、選択中の物件条件名を再度セットする。
                        if(condName == ""){
                            await showAlert("{! MSG_COM_I_E_NG_PROPERTYCONDITIONSNAME_EMPTY}");
								//▼ 2022.01.21 No.307 対応で選択中の物件条件名の取得元を変更
                                document.getElementById('{!$Component.chageCondName}').value = tmpName;
								//▲ 2022.01.21 No.307 対応で選択中の物件条件名の取得元を変更
                                return;
                            return
                        }
                    }

					//▼ 2022.01.21 No.307 対応 末尾の空白を削除し全角英数文字を半角に変換する。
					function zenkakuChangeHankaku(str) {
						return str.trim().replace(/[Ａ-Ｚａ-ｚ０-９]/g, function(s) {
							return String.fromCharCode(s.charCodeAt(0) - 0xFEE0);
						//▼ 2022.01.27 No.307 対応 セミコロンを全角に変換する
						}).replace(/;/g, '；');
						//▲ 2022.01.27 No.307 対応 セミコロンを全角に変換する
					}
					//▲ 2022.01.21 No.307 対応 末尾の空白を削除し全角英数文字を半角に変換する。

