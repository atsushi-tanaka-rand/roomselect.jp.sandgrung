    <apex:outputPanel id="js">

      <script language="javascript" type="text/javascript">
        //変更フラグTrue処理
        function changeFlagSet() {
          $('.changeFlag').val(true);//変更フラグ
        }
          
        // 帳票ロゴ表示エラー時に次画面パラメータを変更
        function onErrorFormLogo() {
          $('.formLogoIsAlive').val('false');
        }
      </script>

      <script language="javascript" type="text/javascript">
          // ｐｄｆ出力
          async function pdfOutput(){

            var saveFlag = "{! saveFlg}";
            var outputDate ="{!parameter.SavedOutputFlag}"; // 保存されたデータから出力されているか判定する用

            // 変更、もしくは保存内容では出していない場合
            if(($('.changeFlag').val() == 'true' )  || saveFlag == 'false' || outputDate == 'false' ){
              // 保存処理の実行
              var message = "{! MSG_COM_V_SAVE}";
            
              var retVal = await showConfirm(message);
              if (!retVal) {
                  return false;
              }
              
              blockUi();
              upsertDatePDF();

              return true;
              
            }           
            // 帳票出力
            showPage('TA102001View', '', '', 'Id={!screenProvisionalQuoteRecord.Id}&formLogoIsAlive=' + $('.formLogoIsAlive').val());
            //showPage('TA102001View', '', 'false', 'Id={!screenProvisionalQuoteRecord.Id}');
          }

          //登録確認メッセージ
          async function checkInsert() {
            
            var message = "{! MSG_COM_V_SAVE}";
            
            var retVal = await showConfirm(message);
            if (!retVal) {
                return false;
            }
            
            blockUi();
            upsertDate();
            return true;
          }

          // 登録完了メッセージ
          async function alertSaveFinish(){
              
              //完了メッセージ
              await showAlert("{!MSG_COM_I_SAVE}");
              $('.changeFlag').val(false);//変更フラグ

              reLoadPage(false); //問合せNo0531で追加

          } 

          // 登録完了メッセージ（PDF押下時）
          async function alertSaveFinishPDF(){
              
              //完了メッセージ
              await showAlert("{!MSG_COM_I_SAVE}");
              $('.changeFlag').val(false);//変更フラグ
              reLoadPage(true); //問合せNo0531で追加
          } 

          //▼問合せNo0531で追加
          // 画面を別タブで開きなおす（ブラウザバックによる画面が新規登録時の画面になることを防ぐ）
          function reLoadPage(isPdfFlg){
            if(!{! parameter.SavedOutputFlag}){
              showPage('GA102002View2', '', 'true', 
                    'Id={!parameter.Id}'
                  + '&SavedOutputFlag=true');
            }
            // 帳票出力
            if(isPdfFlg) {
